  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@prisma/cli": "^2.0.0-beta.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
